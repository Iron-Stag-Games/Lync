{
	"name": "Example Build",
	"build": "BuildScripts/Build/Build.rbxl",
	"port": 34873,
	"collisionGroups": "collisionGroups.json",
	"sourcemapEnabled": {
		"RBXM": true,
		"RBXMX": true
	},

	"tree": {
		"Workspace": {
			"$properties": {
				"FallenPartsDestroyHeight": -1337,
				"Gravity": 133.7
			},

			"$path": "Assets/Workspace",

			"Terrain": {
				"$terrainRegion": [ "Assets/TerrainRegion.rbxm", [ "Vector3int16.new(-32000, -32000, -32000)" ], true ],
				"$terrainMaterialColors": {
					"Grass": [ "Color3.new(0, 0.5, 0)" ]
				}
			},

			"Sample Packages": {
				"Pronghorn": {
					"$path": { "package": "Iron-Stag-Games/Pronghorn", "type": "repo" }
				},
	
				"goodsignal": {
					"$path": { "package": "stravant/goodsignal", "type": "repo" }
				},
	
				"roblox-lua-promise": {
					"$path": { "package": "evaera/roblox-lua-promise", "type": "lua" }
				},
	
				"Fusion": {
					"$path": { "package": "dphfox/Fusion@v0.2-beta", "type": "rbxm" }
				},

				"Janitor": {
					"$path": { "package": "howmanysmall/janitor", "type": "rbxmx" }
				}
			}
		},

		"Lighting": {
			"$properties": {
				"Technology": [ "Enum.Technology.Future" ]
			}
		}
	}
}