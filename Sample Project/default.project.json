{
	"name": "Example Build",
	"port": 34873,
	"base": "default.base.rbxl",
	"build": "BuildScripts/Build/Build.rbxl",
	"sourcemapEnabled": {
		"RBXM": true,
		"RBXMX": true
	},

	"tree": {
		"Workspace": {
			"$properties": {
				"FallenPartsDestroyHeight": -1337,
				"Gravity": 133.7
			},

			"$path": "Assets/Workspace",

			"Terrain": {
				"$terrainRegion": ["Assets/TerrainRegion.rbxm", ["Vector3int16.new(-32000, -32000, -32000)"], true],
				"$terrainMaterialColors": {
					"Grass": ["Color3.new(0, 0.5, 0)"]
				}
			},

			"Sample Packages": {
				"Knit": {
					"$path": { "package": "Sleitnick/Knit" }
				},
	
				"goodsignal": {
					"$path": { "package": "stravant/goodsignal" }
				},
	
				"roblox-lua-promise": {
					"$path": { "package": "evaera/roblox-lua-promise" }
				},
	
				"Fusion": {
					"$path": { "package": "dphfox/Fusion@v0.2-beta" }
				}
			}
		}
	}
}